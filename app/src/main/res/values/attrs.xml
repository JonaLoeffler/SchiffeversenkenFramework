<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="com.jona.schiffeversenken.Ship">
        <attr name="layout_type">
            <enum name="tile" value="0" />
            <enum name="aircraftcarrier" value="-1" />
            <enum name="battleship" value="-2" />
            <enum name="cruiser_1" value="-3" />
            <enum name="cruiser_2" value="-4" />
            <enum name="destroyer" value="-5" />
        </attr>
        <attr name="ship_orientation">
            <enum name="north" value="0" />
            <enum name="east" value="1" />
        </attr>
        <attr name="position" format="integer"/>
    </declare-styleable>

</resources>